    for (i = 0; i < max; i++)
    {
        t1 = get_tsc();
        thread1 = kthread_create(thread_fn,NULL,our_thread);
        t2 = get_tsc();
        printk("TAO:%d:kthread_create_time=%ld\n", i,(t2 - t1));
    }
	
	
	
